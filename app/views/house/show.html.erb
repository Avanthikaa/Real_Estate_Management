<h2>Real Estate Company and its details</h2>
<% @house = House.all %>
<% @house.each do |house| %>
<p>
  <strong>House Id:</strong>
  <%= house.id %>
</p>

<p>
  <strong>Realestate Id:</strong>
  <%= house.realestateid %>
</p>

<p>
  <strong>Location:</strong>
  <%= house.location %>
</p>

<p>
  <strong>squarefootage:</strong>
  <%= house.squarefootage %>
</p>

<p>
  <strong>Year:</strong>
  <%= house.year %>
</p>

<p>
  <strong>Style:</strong>
  <%= house.style %>
</p>

<p>
  <strong>List Price:</strong>
  <%= house.list_price %>
</p>

<p>
  <strong>Floors:</strong>
  <%= house.floors %>
</p>

<p>
  <strong>Basement:</strong>
  <%= house.basement %>
</p>

<p>
  <strong>Current Owner:</strong>
  <%= house.currentowner %>
</p>

<p>
  <strong>Contact:</strong>
  <%= house.contact %>
</p>

<p>
  <strong>Potential Buyers:</strong>
  <%= house.potentialbuyers %>
</p>
<% end %>


<%= link_to 'Back', root_path, :class => 'button' %>
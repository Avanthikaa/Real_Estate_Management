<body data-turbolinks = "false">
<h2> Welcome, <%= current_user.name%></h2>
<br>
<br>

<% if current_user.user_type === 'Admin' %>
  <h4><%= link_to "Real Estate Companies", realestate_show_path%></h4>
  <h4><%= link_to "Create Realtor/House Hunter", new_user_path %></h4>
<% end %>
<h4><%= link_to "Houses", house_show_path%></h4>
<% if current_user.user_type === 'Admin' %>
  <h4><%= link_to "Users", user_show_path%></h4>
<% end %>
<% if current_user.user_type === 'Realestate Hunter' %>
  <h4><%= link_to "Post Inquiries", new_inquiry_path %></h4>
<% end %>
<% if current_user.user_type === 'Admin' || current_user.user_type === 'Realtor' %>
  <h4><%= link_to "View Inquiries", house_path %></h4>
<% end %>
<% if current_user.user_type === 'Realtor' %>
  <h4><%= link_to "Edit Company Details", realestate_edit_path(current_user.company_name.to_i) %></h4>
<% end %>
</body>

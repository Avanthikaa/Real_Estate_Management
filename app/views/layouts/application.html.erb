<!DOCTYPE html>
<html>
<head>
  <title>HouseRental</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
  <!-- current_user will return true if a user is logged in -->
  <%= "Logged in as #{current_user.email}" %> | <%= link_to 'Home', user_menu_path %> | <%= link_to 'Log Out', logout_path, method: :delete %> | <%= link_to "Edit Profile", user_edit_path(current_user) %>
<% else %>
  <!-- not logged in -->
  <%= link_to 'Home', root_path %> | <%= link_to 'Log In', login_path %> or <%= link_to 'Sign Up', new_user_path %>
<% end %>

<hr>

<% if notice %>
  <%= notice %>
<% end %>

<% if alert %>
  <%= alert %>
<% end %>



<%= yield %>

</body>
</html>
